{
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "react-native"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb-base"
  ],
  "rules": {
    "arrow-body-style": "warn",
    "no-use-before-define": 0,
    "class-methods-use-this":0,
    "global-require": 0,
    "linebreak-style": 0
  }
}